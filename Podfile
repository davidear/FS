target 'FS' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  pod 'AFNetworking'
  pod 'MJRefresh'
  pod 'SDWebImage'
  pod 'MBProgressHUD'
  pod 'SVPullToRefresh'
  pod 'Masonry'
  pod 'JSONModel'
  pod 'HMSegmentedControl'
  
  
  # Pods for FS
  target 'FSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
